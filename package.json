{
  "name": "ibooks-notes-sync-cli",
  "version": "0.1.0",
  "description": "CLI for syncing iBooks notes",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "check": "npm run lint && npm run typecheck && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "private": true,
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^25.3.1",
    "eslint": "^10.0.2",
    "globals": "^17.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.1"
  }
}
